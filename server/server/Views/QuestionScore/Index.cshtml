@model IEnumerable<server.Models.QuestionScore>

@{
    ViewData["Title"] = "Index";
}
<style>
    h1 { color: #FFFFFF; margin-left:32px; margin-top:168px}
    create {margin-left:32px}
    card {margin-left:32px}
</style>
<div class="col-md-12 body">
    <br/>
    <h1>QuestionScore</h1>
</div>
<div>
    <br/>
    <create>
        <a asp-action="Create">Create New</a>
    </create>
    <br/>
    <div class="col-md-8 card" style="background:#F4F4F4;">
        <br/>
        <table class="table">
            <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.AnswerTime)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.IdentityUser)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Question)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CourseSection)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Course)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Score)
                </th>
                <th></th>
            </tr>
            </thead>
            <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.AnswerTime)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.IdentityUser.Id)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Question.Id)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.CourseSection.CourseSectionID)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Course.CourseID)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Score)
                    </td>
                    <td>
                        <a asp-action="Edit" asp-route-id="@item.QuestionScoreID">Edit</a> |
                        <a asp-action="Details" asp-route-id="@item.QuestionScoreID">Details</a> |
                        <a asp-action="Delete" asp-route-id="@item.QuestionScoreID">Delete</a>
                    </td>
                </tr>
            }
            </tbody>
        </table>
    </div>
</div>